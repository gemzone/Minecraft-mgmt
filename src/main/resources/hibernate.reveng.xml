<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
	 	<sql-type jdbc-type="NVARCHAR" not-null="false" hibernate-type="String" />
	 	<sql-type jdbc-type="NVARCHAR" not-null="true" hibernate-type="string" />
	 	
	 	<sql-type jdbc-type="BIGINT" not-null="false" hibernate-type="Long" />
	 	<sql-type jdbc-type="BIGINT" not-null="true" hibernate-type="long" />
	</type-mapping>
</hibernate-reverse-engineering>